page {
font-size: 30rpx;
}

.userMobile, .vCode, .password {
  display: flex;
  background: #fff;
  padding: 0 50rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.userMobile {
  padding-right: 20rpx;
}

.userMobile text, .vCode text, .password text {
  flex: 1;
  line-height: 90rpx;
  text-align: center;
}


.userMobile input, .vCode input, .password input {
  height: 90rpx;
  flex: 4;
}

.userMobile input {
  flex: 3;
}

.button-hover[type="primary"] {
  background-color: #20a0ff;
}

.register .submit {
  margin: 40rpx 50rpx 0;
  background-color: #01BBD2;
  letter-spacing: 10rpx;
  line-height: 80rpx;
}


.orderClass {
  margin: 16rpx 0% 0% 0%;
  height: 80rpx;
 font-size: 30rpx;
  line-height: 80rpx;
  margin-top: 10rpx;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 0 50rpx;
}


.nameImage {
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  line-height: 80rpx;
  width: 40rpx;
  height: 40rpx;
  float: right;
}

.select_one{
  margin-top: 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
.m_left{
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
}